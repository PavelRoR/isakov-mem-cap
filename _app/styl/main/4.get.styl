#get
	padding 50px 0
.get-item
	width 100%
	max-width 380px
	margin 0 auto 50px
	padding 200px 0 0
	box-shadow: 0px 4px 5px 4px rgba(25, 24, 21, 0.1);
	display: table
	background-repeat no-repeat
	background-position: center top
	background-size: 100% 200px
	&-1
		background-image url(../img/images/get-img-1.jpg)
	&-2
		background-image url(../img/images/get-img-2.jpg)
	&-3
		background-image url(../img/images/get-img-3.jpg)
	&-4
		background-image url(../img/images/get-img-4.jpg)
	&-5
		background-image url(../img/images/get-img-5.jpg)
	&-6
		background-image url(../img/images/get-img-6.jpg)
	&-text
		text-align: center
		text-transform: uppercase
		font-family: AB
		height 140px
		display: table-cell
		vertical-align: middle
		padding 15px 30px
	